<!-- BEGIN_INCLUDE(ldap:lookup) -->
    <ldap:lookup dn="#[header:dn]">
        <ldap:attributes ref="#[header:attributes]" />
    </ldap:lookup>
<!-- END_INCLUDE(ldap:lookup) -->

<!-- BEGIN_INCLUDE(ldap:search) -->
    <ldap:search baseDn="#[header:baseDn]"
